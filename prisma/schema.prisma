generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Key {
  id          Int      @id @default(autoincrement())
  fingerprint String   @unique
  key         String
  name        String
  email       String
  comment     String
  createdAt   DateTime @default(now())
  expired     Boolean  @default(false)
}
