generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model PublicKey {
  id          Int       @id @default(autoincrement())
  fingerprint String    @unique
  algo        Int?
  length      Int?
  createdAt   DateTime  @default(now())
  expiredAt   DateTime?
  revoked     Boolean   @default(false)
  name        String
  email       String
  comment     String
  armoredKey  String
}
