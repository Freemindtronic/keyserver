{
  "name": "keyserver",
  "description": "Custom Node.js implementation of the OpenPGP HTTP Keyserver Protocol (HKP)",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/Freemindtronic/keyserver/issues"
  },
  "dependencies": {
    "@prisma/client": "2.30.2",
    "debug": "^4.3.2",
    "fast-glob": "^3.2.7",
    "openpgp": "^5.0.0-6"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@types/debug": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "prisma": "2.30.2",
    "sass": "^1.38.2",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.5",
    "svelte-preprocess": "^4.8.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "homepage": "https://github.com/Freemindtronic/keyserver#readme",
  "keywords": [
    "keyserver",
    "pgp"
  ],
  "license": "AGPL-3.0-only",
  "main": "build/index.js",
  "packageManager": "yarn@3.0.1",
  "private": true,
  "repository": "https://github.com/Freemindtronic/keyserver",
  "scripts": {
    "build": "svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev --host --port 11371",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src && eslint --ignore-path .gitignore .",
    "preview": "svelte-kit preview"
  },
  "type": "module",
  "volta": {
    "node": "16.8.0",
    "yarn": "1.22.11"
  }
}
